code/Haskell/
cd code
git filter-branch --force --index-filter 'git rm --cached --ignore-unmatch Haskell/IRC2TelegramM.hs' --prune-empty --tag-name-filter cat -- --all

git push code bla2 --force
rm -rf .git/refs/original/
git reflog expire --expire=now --all
git gc --prune=now

